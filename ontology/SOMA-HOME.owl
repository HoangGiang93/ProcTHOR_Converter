        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Table"/>
        <rdfs:comment>A small table typically located next to a bed.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#Blade -->


