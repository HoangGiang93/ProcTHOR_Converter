    <!-- http://www.ease-crc.org/ont/SOMA.owl#Affordance -->


    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Affordance"/>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#AffordsBeingSitOn -->


    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#AffordsBeingSitOn">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Affordance"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#describes"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#CanBeSatOn"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#definesBearer"/>
                <owl:allValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#SittingDestination"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#definesTrigger"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ease-crc.org/ont/SOMA.owl#AgentRole"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies"/>
                                <owl:allValuesFrom>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#hasDisposition"/>
                                        <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#CanSit"/>
                                    </owl:Restriction>
                                </owl:allValuesFrom>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#definesTask"/>
                <owl:allValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Sitting"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A reified relation between a furniture-like object (the bearer), the sitting task it affords, and an agent that can assume a sitting pose (trigger).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#AgentRole -->


    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#AgentRole"/>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#Appliance -->


    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Appliance">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#DesignedArtifact"/>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#Armchair -->


    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Armchair">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#DesignedChair"/>
        <rdfs:comment>A more comfortable chair that includes sides to support arms.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#AssumingPose -->


    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#AssumingPose"/>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#BakedGood -->


    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#BakedGood">
